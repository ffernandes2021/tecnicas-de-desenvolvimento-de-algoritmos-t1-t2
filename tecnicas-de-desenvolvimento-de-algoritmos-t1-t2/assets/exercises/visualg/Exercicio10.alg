algoritmo "Exercicio10"
    var ano, mes, dia: inteiro
inicio
    escreva("Digite a idade em anos: ")
    leia(ano)
    escreva("Digite a idade em meses: ")
    leia(mes)
    escreva("Digite a idade em dias: ")
    leia(dia)
    escreva("Idade em dias: ", ano * 365 + mes * 30 + dia)
fimalgoritmo
